*{
	margin: 0;
	padding: 0;
}
body{
	background: #F4F4F4;
	font-size: 1.5rem;
}
.box{
	width: 100%;
	background: #FFFFFF;
}
h5.hed{
	color: #999999;
	padding: 4% 3%;
	border-bottom:1px solid #eaeaea;
	margin-bottom: 0;
}
li{
	list-style: none;
	height: 50px;
	border-bottom:1px solid #F1F1F1;
	line-height: 50px;
	padding-right: 3%;
	margin-left: 3%;
	width: 97%;
}
li span{
	width: 20%;
	float: left;
}
li img.tx{
	float: right;
	width: 30px;
	margin: 10px 0;
}
li input{
	background: url(../images/zhankai.png) no-repeat right center;
	height: 40px;
	line-height: 40px;
	border:0 ;
	float: right;
	width: 60%;
	padding-right: 5%;
	text-align: right;
	color: #666666;
}
li label,li select{
	float: right;
	font-weight:normal;
	margin: 0;
	color: #666666;
}
li select{
	border:0 ;
	height: 30px;
    margin: 10px 0;
    line-height: 30px;
    appearance:none;  
    -moz-appearance:none;  
    -webkit-appearance:none;
     background: url("../images/zhankai.png") no-repeat scroll right center transparent;
     padding-right: 14px;
}
.row {
	width: 60%;
	float: right;
	margin: 0;
}

.row input {
	width: 100%;
	height: 40px;
	padding-right: 8%;
}
.wheel-scroll.wheel-scroll-hook {
            margin: 0;
            padding: 0;
        }
        #picker{
            
        }
li.wheel-item {
        	line-height: 36px;
        	border: 0;
        }